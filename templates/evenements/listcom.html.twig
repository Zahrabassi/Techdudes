{% block body %}
<h1>liste des participant </h1>

                <table border="2" class="table table-striped table-advance table-hover">

                    <thead>
                    <tr>
                        <th>id</th>
                        <th>nom</th>
                        <th>date allee</th>
                        <th>date retour</th>
                        <th>description</th>

                    </tr>
                    </thead>

                    {% for i in eve %}
                        <tr>
                            <td>{{ i.id}}</td>
                            <td>{{ i.nom}}</td>
                            <td>{{ i.date_allee}}</td>
                            <td>{{ i.date_retour}}</td>
                            <td>{{ i.description}}</td>
                        </tr>
                    {% endfor %}


                </table>

{% endblock %}