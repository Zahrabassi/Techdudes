        <script src="{{ asset('js/gijgo.min.js') }}"></script>



        <!--contact js-->
        <script src="{{ asset('js/contact.js') }}"></script>
        <script src="{{ asset('js/jquery.ajaxchimp.min.js') }}"></script>
