<body>
{% block body %}

<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
<![endif]-->
<!-- header-start -->
<header>
    <div class="header-area ">
        <div id="sticky-header" class="main-header-area">
            <div class="container-fluid ">
                <div class="header_bottom_border">
                    <div class="row align-items-center">
                        <div class="col-xl-3 col-lg-2">
                            <div class="logo">
                                <a href= {{ path('mescour') }}>
                                    <img src="{{ asset('img/ka_logo.png') }}" alt="">
                                </a>
                            </div>
                        </div>
                        <div class="col-xl-6 col-lg-7">
                            <div class="main-menu  d-none d-lg-block">
                                <nav>
                                    <ul id="navigation">

                                        <li><a href= {{ path('listCour') }}>Cours</a></li>
                                        <li><a href={{ path('listformation') }}>Formation</a></li>
                                        <li><a href={{ path('evenements') }}>Evenement</a></li>
                                        <li><a href="Reclamation.html">Reclamation</a></li>
                                        {% block usr %}
                                            {% if is_granted('ROLE_ETUDIANT') %}

                                                <li class="nav-item" ><a class="nav-link" href="{{ path('dash') }}">{{ app.user.name }}</a>
                                                </li>
                                                <li class="nav-item"><a class="nav-link"
                                                                        href="{{ path('security_logout') }}">{{ 'user.logout'|trans }}</a></li>

                                            {% elseif is_granted('ROLE_ENSEIGNANT') %}

                                                <li class="nav-item" ><a class="nav-link"  href="{{ path('user_edit') }}">{{ app.user.name }}</a>
                                                </li>
                                                <li class="nav-item"><a class="nav-link"
                                                                        href="{{ path('security_logout') }}">{{ 'user.logout'|trans }}</a></li>
                                            {% elseif is_granted('ROLE_ADMIN') %}

                                                <li class="nav-item" ><a class="nav-link"  href="{{ path('user_edit') }}">{{ app.user.name }}</a>
                                                </li>
                                                <li class="nav-item"><a class="nav-link"
                                                                        href="{{ path('security_logout') }}">{{ 'user.logout'|trans }}</a></li>
                                            {% else %}
                                                {#<li><a href="{{ path('user_register') }}">{{ 'user.sign-up'|trans }}</a></li>#}
                                                <li class="nav-item"><a class="nav-link"
                                                                        href="{{ path('security_login') }}">{{ 'user.login'|trans }}</a></li>

                                            {% endif %}
                                        {% endblock %}


                                    </ul>

                                </nav>

                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                            <div class="Appointment">
                                <div class="d-none d-lg-block">
                                    <a class="boxed-btn3" href="{{ path('cart') }}">Pannier</a>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>
</header>
<div class="slider_area">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-7 col-md-6">

                </div>
            </div>
    </div>

</div>
<div class="tz-site">

    <!--Start id tz header-->

        <div class="container">

            <!--Start class Shortcode tabs-->
            <div class="tz-shortcode-tabs">



                <section class="default-page">
                    <div class="container">
                        <div class="tz-register">
                            <h2>participer</h2>


                            {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
                            <div class="form-row form-row-wide">
                                {{ form_label(form.nom,"Nom") }}
                                {{ form_widget(form.nom ,{'attr':{'class': 'form-row form-row-wide'}} ) }}
                            </div>
                            <div class="form-row form-row-wide">
                                {{ form_label(form.prenom,"prenom") }}
                                {{ form_widget(form.prenom , {'attr':{'class': 'input-text'}}) }}
                            </div>
                            <div class="form-row form-row-wide">
                                {{ form_label(form.email,"email") }}
                                {{ form_widget(form.email , {'attr':{'class': 'input-text'}}) }}
                            </div>
                            <div class="form-row form-row-wide">
                                {{ form_label(form.numtel,"numtel") }}
                                {{ form_widget(form.numtel , {'attr':{'class': 'input-text'}}) }}
                            </div>


                            <p class="form-row">
                                <input type="submit" class="button" name="register" value="participer">
                            </p>
                            {{ form_end(form) }}
                        </div>
                    </div>
                </section>




            <!--End class site-->
            {% block js %}
                <script type='text/javascript' src="js/jquery.min.js"></script>
                <script type='text/javascript' src="js/bootstrap.min.js"></script>
                <script type='text/javascript' src="js/off-canvas.js"></script>
                <!--jQuery Countdow-->
                <script type='text/javascript' src="js/jquery.plugin.min.js"></script>
                <script type='text/javascript' src="js/jquery.countdown.min.js"></script>
                <!--End Countdow-->
                <script type='text/javascript' src="js/jquery.parallax-1.1.3.js"></script>
                <script type='text/javascript' src="js/owl.carousel.js"></script>
                <script type='text/javascript' src="js/custom.js"></script>
                <script type='text/javascript' src='rs-plugin/js/jquery.themepunch.tools.min.js'></script>
                <script type='text/javascript' src='rs-plugin/js/jquery.themepunch.revolution.min.js'></script>
                <script type='text/javascript' src='rs-plugin/js/custom-rs.js'></script>
                <script src="js/html5shiv.min.js"></script>
                <script src="js/respond.min.js"></script>
            {% endblock %}
            {% endblock %}